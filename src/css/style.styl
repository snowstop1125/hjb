//  checkbox样式
.chk {
  display: none;
}
.chk + label {
  background-color: #FFF;
  border: 2px solid #ff612a;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05);
  width: 35px;
  height: 35px;
  border-radius: 5px;
  display: inline-block;
  position: relative;
}
.chk + label:active {
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px 1px 3px rgba(0, 0, 0, 0.1);
}
.chk:checked + label {
  background-color: #ff612a;
  border: 2px solid #ff612a;
  color: #243441;
}
.chk:checked + label:after {
  content: '\2714';
  position: absolute;
  top: -1px;
  left: 0px;
  color: #fff;
  width: 100%;
  text-align: center;
  font-size: 28px;
  vertical-align: text-top;
  line-height: 35px;
}
